#include <iostream>
#include <cmath>
using namespace std;
int main(){
    double a, b, x, y;
    cout<<"Введите a: ";
    cin>>a;
    cout<<"Введите b: ";
    cin>>b;
    x=((2/(pow(a, 2))+(25))+b)/(sqrt(b)+((a+b)/2));
    y=(abs(a)+(2*sin(b)))/5.5*a;
    cout<<"x = "<<x<<endl;
    cout<<"y = "<<y<<endl;
    return 0;
}
