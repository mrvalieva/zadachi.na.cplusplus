#include <iostream>
#include <cmath>
using namespace std;
int main(){
    double x1, x2, y1, y2, h;
    cout<<"Введите координаты первой точки: ";
    cin>>x1>>y1;
    cout<<"Введите координаты второй точки: ";
    cin>>x2>>y2;
    h=sqrt((x2-x1)*(x2-x1)+(y2-y1)*(y2-y1));
    cout<<"Расстояние = "<<h<<endl;
    return 0;
}
