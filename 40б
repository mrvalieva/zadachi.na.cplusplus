#include <iostream>
#include <cmath>
using namespace std;
int main(){
    double a, b, c;
    cout<<"Введите a: ";
    cin>>a;
    cout<<"Введите b: ";
    cin>>b;
    cout<<"Введите c: ";
    cin>>c;
    swap(a, c);
    swap(b, a);
    swap(c, b);
    cout<<"a = "<<c<<endl;
    cout<<"b = "<<b<<endl;
    cout<<"c = "<<a<<endl;
    return 0;
}
