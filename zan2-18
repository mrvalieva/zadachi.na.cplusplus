#include <iostream>
using namespace std;

int main() {
    int a, b, c;
    cout << "Введите число A: ";
    cin >> a;
    cout << "Введите число B: ";
    cin >> b;
    cout << "Введите число C: ";
    cin >> c;

    if (a == b || a == c || b == c) {
        cout << "Среди трех данных целых чисел есть хотя бы одна пара совпадающих";
    } else {
        cout << "Среди трех данных целых чисел нет пары совпадающих";
    }

    return 0;
}
