#include <iostream>
using namespace std;

int main() {
    int a, b, c;
    cout << "Введите три числа: ";
    cin >> a >> b >> c;

    int max = a;
    int min = a;
    if (b > max) {
        max = b;
    } else if (b < min) {
        min = b;
    }
    if (c > max) {
        max = c;
    } else if (c < min) {
        min = c;
    }

    cout << "Наименьшее число: " << min << endl;
    cout << "Наибольшее число: " << max;

    return 0;
}
