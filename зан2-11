#include <iostream>
using namespace std;

int main() {
    int a, b;
    cout << "Введите число A: ";
    cin >> a;
    cout << "Введите число B: ";
    cin >> b;

    if ((a % 2 == 0 && b % 2 == 0) || (a % 2 != 0 && b % 2 != 0)) {
        cout << "Числа А и В имеют одинаковую четность";
    } else {
        cout << "Числа А и В имеют разную четность";
    }

    return 0;
}
