#include <iostream>
using namespace std;

int main() {
    int a, b, c;
    cout << "Введите число A: ";
    cin >> a;
    cout << "Введите число B: ";
    cin >> b;
    cout << "Введите число C: ";
    cin >> c;

    if (a > 0 && b > 0 && c > 0) {
        cout << "Каждое из чисел А, В, С положительное";
    } else {
        cout << "Не каждое из чисел А, В, С положительное";
    }

    return 0;
}
