#include <iostream>
using namespace std;

int main() {
    int n;
    cout << "Введите число: ";
    cin >> n;

    if (n % 2 != 0 && n >= 100 && n < 1000) {
        cout << "Данное число является нечетным трехзначным";
    } else {
        cout << "Данное число не является нечетным трехзначным";
    }

    return 0;
}
