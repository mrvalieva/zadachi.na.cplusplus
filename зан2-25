#include <iostream>
using namespace std;

int main() {
    int x, y;
    cout << "Введите координаты точки (x, y): ";
    cin >> x >> y;

    if (x < 0 && y > 0) {
        cout << "Точка с координатами (" << x << ", " << y << ") лежит во второй координатной четверти";
    } else {
        cout << "Точка с координатами (" << x << ", " << y << ") не лежит во второй координатной четверти";
    }

    return 0;
}
