#include <iostream>
using namespace std;

int main() {
    int a, b;
    cout << "Введите число A: ";
    cin >> a;
    cout << "Введите число B: ";
    cin >> b;

    if (a >= 0 && b < -2) {
        cout << "Справедливы неравенства A >= 0 и B < -2";
    } else {
        cout << "Неравенства A >= 0 и B < -2 не выполняются";
    }

    return 0;
}
