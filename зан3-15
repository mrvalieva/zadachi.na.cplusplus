#include <iostream>
using namespace std;

int main() {
    int a, b, sum = 0;
    cout << "Введите значение a: ";
    cin >> a;
    cout << "Введите значение b: ";
    cin >> b;

    for (int i = a; i <= b; i++) {
        sum += i;
    }

    cout << "Сумма всех целых чисел от " << a << " до " << b << ": " << sum << endl;

    return 0;
}
