#include <iostream>
#include <cmath>
using namespace std;

int main() {
    int n;
    float sum = 0;
    cout << "Введите количество чисел: ";
    cin >> n;

    for (int i = 1; i <= n; i++) {
        float a;
        cout << "Введите вещественное число a" << i << ": ";
        cin >> a;
        sum += pow(a, 2);
    }

    cout << "Сумма квадратов вещественных чисел: " << sum << endl;

    return 0;
}
