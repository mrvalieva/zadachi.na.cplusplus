#include <iostream>
using namespace std;

int main() {
    int n;
    float sum = 0;
    cout << "Введите количество вещественных чисел: ";
    cin >> n;

    for (int i = 1; i <= n; i++) {
        float a;
        cout << "Введите вещественное число a" << i << ": ";
        cin >> a;
        sum += a;
    }

    float average = sum/n;
    cout << "Среднее арифметическое всех вещественных чисел: " << average << endl;

    return 0;
}
