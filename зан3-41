#include <iostream>
using namespace std;

int main() {
    int a, b;
    cout << "Введите два целых числа (A < B): ";
    cin >> a >> b;

    int count = b - a - 1;
    cout << "Количество чисел между A и B: " << count << endl;

    for (int i = b - 1; i > a; i--) {
        cout << i << " ";
    }

    return 0;
}
