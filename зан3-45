#include <iostream>
using namespace std;

int main() {
    int a, b;
    cout << "Введите два целых числа (A < B): ";
    cin >> a >> b;

    int sum = 0;
    for (int i = a; i <= b; i++) {
        sum += i;
    }

    cout << "Сумма всех целых чисел от A до B включительно: " << sum << endl;

    return 0;
}
