#include <iostream>
using namespace std;

int main() {
    float pricePerKg;
    cout << "Введите стоимость 1 кг сыра: ";
    cin >> pricePerKg;

    for (int i = 50; i <= 1000; i += 50) {
        float total = pricePerKg * i / 1000;
        cout << i << " г сыра - " << total << " руб." << endl;
    }

    return 0;
}
