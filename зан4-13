#include <iostream>
using namespace std;

int main() {
    for (int num = 50; num <= 70; num++) {
        int sum = 0;
        for (int i = 1; i <= num; i++) {
            if (num % i == 0) {
                sum += i;
            }
        }
        cout << "Сумма делителей числа " << num << ": " << sum << endl;
    }

    return 0;
}
