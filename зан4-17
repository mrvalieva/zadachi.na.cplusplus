#include <iostream>
using namespace std;

int main() {
    const int size = 12;
    int arr[size];
    float sum = 0;

    for (int i = 0; i < size; i++) {
        cout << "Введите элемент массива: ";
        cin >> arr[i];
        sum += arr[i];
    }

    arr[4] = sum / size;

    cout << "Новый массив: ";
    for (int i = 0; i < size; i++) {
        cout << arr[i] << " ";
    }

    return 0;
}
