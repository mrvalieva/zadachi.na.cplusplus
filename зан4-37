#include <iostream>
using namespace std;

int main() {
    const int size = 15;
    float arr[size];

    for (int i = 0; i < size; i++) {
        cout << "Введите элемент массива: ";
        cin >> arr[i];
    }

    for (int i = size - 1; i >= 0; i--) {
        cout << arr[i] << " ";
    }

    return 0;
}
