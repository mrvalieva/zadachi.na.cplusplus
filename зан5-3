#include <iostream>
using namespace std;

char compareInts(int a, int b) {
    if (a > b) {
        return '>';
    } else if (a < b) {
        return '<';
    } else {
        return '=';
    }
}

int main() {
    int x = 10;
    int y = 20;
    char sign = compareInts(x, y);
    cout << "Результат сравнения: " << sign << endl;
    return 0;
}
