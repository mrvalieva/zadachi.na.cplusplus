#include <iostream>
using namespace std;

const int N = 3;

int dotProduct(int A[], int B[]) {
    int result = 0;
    for (int i = 0; i < N; i++) {
        result += A[i] * B[i];
    }
    return result;
}

int main() {
    int A[N] = {1, 2, 3};
    int B[N] = {4, 5, 6};
    int result = dotProduct(A, B);
    cout << "Скалярное произведение векторов: " << result << endl;
    return 0;
}
